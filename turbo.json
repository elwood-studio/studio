{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "cache": false,
      "outputs": ["dist/**", "build/**"],
      "dependsOn": ["^build"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {},
    "@elwood/cli#dev": {
      "dependsOn": ["@elwood/sdk#dev"]
    },
    "@elwood/sdk#dev": {
      "dependsOn": ["@elwood/workflow-sdk#dev", "@elwood/fs-sdk#dev"]
    }
  }
}
