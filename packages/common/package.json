{
  "name": "@elwood/common",
  "version": "0.1.22",
  "main": "./src/index.ts",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    },
    "./*.js": "./dist/*.js",
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/esm/*"
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup --silent",
    "lint": "eslint \"**/*.ts*\""
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  },
  "devDependencies": {
    "@elwood/types": "workspace:*",
    "@types/jest": "^29.5.2",
    "jest": "^29.6.0",
    "jest-presets": "workspace:*",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  }
}
