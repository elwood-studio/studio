{
  "name": "@elwood/workflow-service",
  "version": "0.1.15",
  "type": "module",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs",
        "types": "./dist/index.d.ts"
      }
    },
    "./*.js": "./dist/*.js",
    "./*": {
      "require": "./dist/*.cjs",
      "import": "./dist/*.js"
    }
  },
  "scripts": {
    "lint": "eslint \"**/*.ts*\"",
    "dev": "tsup --watch",
    "build": "tsup --silent"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@elwood/types": "workspace:*",
    "@elwood/workflow-config": "workspace:*",
    "@elwood/workflow-runner": "workspace:*",
    "@elwood/workflow-secrets": "workspace:*",
    "@elwood/workflow-types": "workspace:*",
    "@types/node": "^20.3.1",
    "@types/pg": "^8.10.2",
    "@vercel/ncc": "^0.36.1",
    "esbuild": "^0.18.7",
    "rimraf": "^5.0.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig": "workspace:*",
    "tsup": "^7.0.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "fast-json-stringify": "^5.7.0",
    "fastify": "^4.18.0",
    "fastify-plugin": "^4.5.0",
    "glob": "^10.3.0",
    "pg": "^8.11.0",
    "pg-boss": "8.4.2",
    "ssh2": "^1.14.0"
  }
}
