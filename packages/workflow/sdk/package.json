{
  "name": "@elwood/workflow-sdk",
  "version": "0.1.15",
  "main": "./src/index.ts",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    },
    "./*.js": "./dist/*.js",
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/esm/*"
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "rimraf ./dist && tsup --watch",
    "build": "rimraf ./dist && tsup",
    "lint": "eslint \"**/*.ts*\""
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  },
  "devDependencies": {
    "@elwood/types": "workspace:*",
    "@types/crypto-js": "^4.1.1",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jest": "^29.5.2",
    "dotenv": "^16.3.1",
    "jest": "^29.5.0",
    "jest-presets": "workspace:*",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "tsup": "^7.0.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.26.0",
    "crypto-js": "^4.1.1",
    "emittery": "^1.0.1",
    "isomorphic-fetch": "^3.0.0",
    "tus-js-client": "^3.1.0"
  }
}
