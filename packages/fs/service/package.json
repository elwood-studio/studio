{
  "name": "@elwood/fs-service",
  "version": "0.1.25",
  "license": "MIT",
  "type": "module",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs",
        "types": "./dist/index.d.ts"
      }
    },
    "./*.js": "./dist/*.js",
    "./*": {
      "require": "./dist/*.cjs",
      "import": "./dist/*.js"
    }
  },
  "scripts": {
    "lint": "eslint \"src/**/*.ts*\"",
    "dev": "pnpm run \"/^build:.*/\" --watch",
    "build": "pnpm run \"/^build:.*/\"",
    "build:js": "tsup --silent",
    "build:ts": "tsc --project tsconfig.json"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@elwood/types": "workspace:*",
    "@google-cloud/storage": "^6.11.0",
    "@types/auth-header": "^1.0.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/pg": "^8.10.2",
    "@types/uuid": "^9.0.2",
    "@vercel/ncc": "^0.36.1",
    "esbuild": "^0.18.11",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig": "workspace:*",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "pg-boss": "8.4.2",
    "@elwood/common": "workspace:*",
    "@fastify/postgres": "^5.2.0",
    "@fastify/static": "^6.10.2",
    "@fastify/swagger": "^8.8.0",
    "@fastify/swagger-ui": "^1.9.2",
    "@hapi/boom": "^10.0.1",
    "@tus/file-store": "1.0.0-beta.1",
    "@tus/gcs-store": "1.0.0-beta.3",
    "@tus/s3-store": "1.0.0-beta.6",
    "@tus/server": "1.0.0-beta.6",
    "auth-header": "^1.0.0",
    "fastify": "^4.19.2",
    "fastify-boom": "^1.0.0",
    "fastify-plugin": "^4.5.0",
    "jsonwebtoken": "^9.0.1",
    "md5-file": "^5.0.0",
    "pg": "^8.11.1",
    "ts-invariant": "^0.10.3",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "@google-cloud/storage": "^6.11.0"
  }
}
