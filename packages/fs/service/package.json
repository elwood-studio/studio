{
  "name": "@elwood/fs-service",
  "version": "0.1.13",
  "license": "MIT",
  "type": "module",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs",
        "types": "./dist/index.d.ts"
      }
    },
    "./*.js": "./dist/*.js",
    "./*": {
      "require": "./dist/*.cjs",
      "import": "./dist/*.js"
    }
  },
  "scripts": {
    "lint": "eslint \"**/*.ts*\"",
    "dev": "rimraf ./dist && tsup --watch",
    "build": "rimraf ./dist && tsup"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@elwood/types": "workspace:*",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.3.1",
    "@types/pg": "^8.10.2",
    "@vercel/ncc": "^0.36.1",
    "esbuild": "^0.18.6",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig": "workspace:*",
    "tsup": "^7.0.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@fastify/postgres": "^5.2.0",
    "@fastify/static": "^6.10.2",
    "@hapi/boom": "^10.0.1",
    "@tus/file-store": "1.0.0-beta.1",
    "@tus/server": "1.0.0-beta.6",
    "fastify": "^4.18.0",
    "fastify-boom": "^1.0.0",
    "fastify-plugin": "^4.5.0",
    "jsonwebtoken": "^9.0.0",
    "md5-file": "^5.0.0",
    "pg": "^8.11.0",
    "ts-invariant": "^0.10.3"
  }
}
