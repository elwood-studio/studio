{
  "name": "@elwood-studio/cli",
  "version": "0.1.5",
  "type": "module",
  "main": "./dist/index.ts",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    },
    "./*.js": "./dist/*.js",
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/esm/*"
    }
  },
  "bin": {
    "elwood-studio": "./bin/cli.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "test": "jest --config=./config/jest.js",
    "build": "rimraf ./dist && tsup",
    "dev": "tsup --watch",
    "dev-cli": "node --loader ts-node/esm --no-warnings ./bin/cli-dev.ts"
  },
  "dependencies": {
    "@elwood-studio/js": "workspace:*",
    "@iarna/toml": "^2.2.5",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "docker-compose": "^0.24.1",
    "fs-jetpack": "^5.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^9.0.0",
    "stacktracey": "^2.1.8",
    "ts-invariant": "^0.10.2",
    "yaml": "^2.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@elwood-studio/types": "workspace:*",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/yargs": "^17.0.24",
    "pkg": "^5.5.1",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "tsup": "^6.1.3"
  },
  "license": "MIT",
  "gitHead": "48010a3ebcddc947777266e6c4e6907241792fb6"
}
